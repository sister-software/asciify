{
  "name": "@sister.software/asciify",
  "version": "3.0.2",
  "description": "Converts images, videos, and 3D renders into ASCII art.",
  "repository": "git@github.com:sister-software/asciify.git",
  "bugs": {
    "url": "https://github.com/sister-software/asciify/issues"
  },
  "author": "Teffen Ellis <teffen@sister.software>",
  "license": "MIT",
  "private": false,
  "main": "./dist/mod.mjs",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.mts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "ascii",
    "art",
    "image",
    "video",
    "3d",
    "render",
    "three",
    "threejs",
    "rasterizer"
  ],
  "homepage": "https://sister.software/asciify",
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "types": "./dist/mod.d.mts"
    },
    "./mod": {
      "import": "./dist/mod.mjs",
      "types": "./dist/mod.d.mts"
    },
    "./mod.mjs": {
      "import": "./dist/mod.mjs",
      "types": "./dist/mod.d.mts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "build": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm ./build.mts",
    "docs": "typedoc",
    "demo": "http-server ./ -p 8081"
  },
  "devDependencies": {
    "@sister.software/ts-path-transformer": "^1.0.2",
    "@types/node": "^18.15.10",
    "@types/stats": "^0.16.30",
    "@types/three": "^0.148.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "dat.gui": "^0.7.9",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "http-server": "^14.1.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.0",
    "stats-js": "^1.0.1",
    "three": "^0.148.0",
    "ts-node": "^10.9.1",
    "typedoc": "0.23.28",
    "typedoc-github-wiki-theme": "^1.0.1",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=16.14"
  }
}
