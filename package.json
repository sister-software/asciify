{
  "name": "@sister.software/asciify",
  "version": "1.1.4",
  "description": "Converts images, videos, and 3D renders into ASCII art.",
  "repository": "git@github.com:sister-software/asciify.git",
  "bugs": {
    "url": "https://github.com/sister-software/asciify/issues"
  },
  "author": "Teffen Ellis <teffen@sister.software>",
  "license": "MIT",
  "private": false,
  "main": "./dist/mod.mjs",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.mts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "ascii",
    "art",
    "image",
    "video",
    "3d",
    "render",
    "three",
    "threejs",
    "rasterizer"
  ],
  "homepage": "https://asciify.sister.software",
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "types": "./dist/mod.d.mts"
    },
    "./mod": {
      "import": "./dist/mod.mjs",
      "types": "./dist/mod.d.mts"
    },
    "./mod.mjs": {
      "import": "./dist/mod.mjs",
      "types": "./dist/mod.d.mts"
    },
    "./mod.d.mts": {
      "import": "./dist/mod.d.mts",
      "types": "./dist/mod.d.mts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@types/three": "^0.148.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.0",
    "typedoc": "0.23.24",
    "typedoc-github-wiki-theme": "^1.0.1",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.14"
  }
}
